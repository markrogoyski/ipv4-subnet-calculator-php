{
  "name": "markrogoyski/ipv4-subnet-calculator",
  "type": "library",
  "description": "Minimal composer config for CI unit tests on PHP 8.1+",
  "license": "MIT",
  "require": {
    "php": ">=8.1.0",
    "ext-ctype": "*",
    "ext-filter": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "php-parallel-lint/php-parallel-lint": "^1.4"
  },
  "autoload": {
    "psr-4": { "IPv4\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "IPv4\\Tests\\": "tests/" }
  }
}
